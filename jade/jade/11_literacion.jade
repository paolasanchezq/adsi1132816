octype
html(lang="es")
head
  meta(charset="UTF-8")
  title Literación (Jade)
body
  h1 Literación (Jade)
  
ul
  each val in [1, 2, 3, 4, 5]
    li= val
ul
  each val, index in ['zero', 'one', 'two']
    li= index + ': ' + val
ul
  each val, index in {1:'one',2:'two',3:'three'}
    li= index + ': ' + val
- var values = [];
ul
  each val in values.length ? values : ['There are no values']
    li= val
    - var n = 0
ul
  while n < 4
    li= n++